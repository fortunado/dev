#!/bin/sh

# CLO let=all ban=file

in=$(basename $0)
in=${in%.*} 

mkdir slice
$1/hasqd -cqnx lock=1 dprn=1 dsvt=1 tcp_port=14000 s=$in.inp webroot="webroot" skckey="key" let="all" ban="file" | grep -v "<svt> sendnote\|<svt> conupdate\|<svt> reorg\|<svt> noterecv\|<svt> chkconfl\|<svt> setconfl\|<svt> conflict"  

rm -rf slice

#!/bin/sh

# CLO: "let" and "ban" 
# NC: admin

in=$(basename $0)
in=${in%.*} 

echo ................
echo "skckey+"
echo ................

for i in "admin disable net" "admin enable net" "admin reorg" "admin skc add abc" "admin skc pop" #"admin skc show"
do
    inp="""print ex 0 REQ_MSG_HEAD 100 tcp self [ "$i" ];quit"""
    $1/hasqd -clqnx dprn=1 dsvt=1 s=@"$inp" skckey="key"
done

echo ................
echo "skckey+ let+"
echo ................

for i in "admin disable net" "admin enable net" "admin reorg" "admin skc add abc" "admin skc pop" #"admin skc show"
do
    inp="""print ex 0 OK 100 tcp self [ "$i" ];quit"""
    $1/hasqd -clqnx dprn=1 dsvt=1 s=@"$inp" skckey="key" let="admin" | grep -v "<svt> reorg\|<svt> conupdate"
done

echo ................
echo "skckey+ let=all"
echo ................

for i in "admin disable net" "admin enable net" "admin reorg" "admin skc add abc" "admin skc pop" #"admin skc show"
do
    inp="""print ex 0 OK 100 tcp self [ "$i" ];quit"""
    $1/hasqd -clqnx dprn=1 dsvt=1 s=@"$inp" skckey="key" let="all" | grep -v "<svt> reorg\|<svt> conupdate"
done

echo ................
echo "skckey+ ban+"
echo ................

for i in "admin disable net" "admin enable net" "admin reorg" "admin skc add abc" "admin skc pop" #"admin skc show"
do
    inp="""print ex 0 REQ_MSG_HEAD 100 tcp self [ "$i" ];quit"""
    $1/hasqd -clqnx dprn=1 dsvt=1 s=@"$inp" skckey="key" ban="all"
done

echo ................
echo "skckey+ ban=all"
echo ................

for i in "admin disable net" "admin enable net" "admin reorg" "admin skc add abc" "admin skc pop" #"admin skc show"
do
    inp="""print ex 0 REQ_MSG_HEAD 100 tcp self [ "$i" ];quit"""
    $1/hasqd -clqnx dprn=1 dsvt=1 s=@"$inp" skckey="key" ban="all"
done

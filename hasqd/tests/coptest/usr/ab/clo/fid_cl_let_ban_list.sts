#!/bin/sh

# CLO: "let" and "ban" 
# NC: list
in=$(basename $0)
in=${in%.*} 
inp0="""print ex 0 OK 100 tcp self [ list ];quit"""
inp1="""print ex 0 REQ_MSG_HEAD 100 tcp self [ list ];quit"""

rm -fr dbs
mkdir dbs
mkdir dbs/home
mkdir dbs/home/webdir
cd dbs
$1/zdb erasedisk "create wrd.db wrd Word 1 [] 1 0"
$1/zdb load "add wrd.db 0 0cc1 0000 2e6c adb2"
cd ..

echo ................
echo "skckey+"
echo ................
$1/hasqd -clqnx db="dbs" dprn=1 dsvt=1 skckey="abc" s=@"$inp0"

echo ................
echo "skckey+ let+"
echo ................
$1/hasqd -clqnx db="dbs" dprn=1 dsvt=1 skckey="abc" s=@"$inp0" let="list"

echo ................
echo "skckey+ ban+"
echo ................
$1/hasqd -clqnx db="dbs" dprn=1 dsvt=1 skckey="abc" s=@"$inp1" ban="list"

rm -rf dbs
